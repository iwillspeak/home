{
    "explorer.confirmDragAndDrop": false,
    "workbench.startupEditor": "newUntitledFile",
    "extensions.ignoreRecommendations": false,
    "git.autofetch": true,
    "editor.rulers": [
        80,
        120
    ],
    "git.confirmSync": false,
    "todohighlight.include": [
        "**/*.cs",
        "**/*.json",
        "**/*.xml"
    ],
    "todohighlight.toggleURI": true,
    "cSpell.userWords": [
        "ACKed",
        "Lucene",
        "Memoisable",
        "Nuget",
        "Octopack",
        "Slugified",
        "Slugifies",
        "Slugify",
        "Tokeniser",
        "Wrappable",
        "amet",
        "deserialise",
        "deserialised",
        "deserialistation",
        "memoise",
        "rebuffer",
        "recurse",
        "requeing",
        "serialiser",
        "toolchain",
        "unwrappable",
        "uploader",
        "upsert",
        "xunit",
        "zorgotron"
    ],
    "cSpell.language": "en,en-GB",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.renderWhitespace": "all",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "Menlo, Monaco, 'DejaVu Sans Mono', Consolas, 'Courier New', monospace",
    "debug.onTaskErrors": "showErrors",
    "razor.disabled": true,
    "git.autoStash": true,
    "git.enableSmartCommit": true,
    "workbench.statusBar.visible": true,
    "FSharp.useSdkScripts": true,
    "terminal.integrated.allowChords": false,
    "emacs-mcx.strictEmacsMove": true,
    "editor.cursorWidth": 4,
    "gtkTitleBar.mode": "dark",
    "emacs-mcx.useMetaPrefixMacCmd": true,
    "omnisharp.useModernNet": true,
    "workbench.colorTheme": "Night Owl",
    "redhat.telemetry.enabled": true,
    "FSharp.suggestGitignore": false,
    "workbench.editor.showTabs": "single",
    "window.menuBarVisibility": "toggle",
    "editor.bracketPairColorization.enabled": false,
    "FSharp.inlayHints.parameterNames": false,
    "FSharp.inlayHints.typeAnnotations": false,
    "FSharp.inlayHints.enabled": false,
    "FSharp.dotnetRoot": "/usr/bin",
    "settingsSync.ignoredSettings": [
        "FSharp.dotnetRoot"
    ],
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "dotnet.formatting.organizeImportsOnFormat": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "workbench.secondarySideBar.defaultVisibility": "hidden"
}
